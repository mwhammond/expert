
<div class="row centered">

  <% if startups %>
  <% startups.each do |startup| %>

  <div class="featuredpanel">

    <%= link_to image_tag(startup.avatar.url(:medium), class: "img-responsive"), startup %>
    <div>
      <div class="featuredpanel-inner">

        <h3><%= link_to startup.name, startup%></h3>

        <p><b><%= startup.sector %></b><p>
          <p><%= startup.bio %></p>
          <p><b>Searching for: </b><%= startup.requests %></p>
          <p><b>Supporters: </b><a href="#">Kelsey Lyn, Mark Hammond, Tony Williams</a></p>
          <div class="featuredpanel-bottom">
            <div class="row centered">

              
                <%= link_to "Connect", new_startup_path, :class => "btn btn-primary btn-sm centered", method: :update %>

              
            </div>
          </div>

          <div>

          </div>
        </div>
      </div>
      <br>
    </div>

    <%end%>
    <%end%>

  </div>

